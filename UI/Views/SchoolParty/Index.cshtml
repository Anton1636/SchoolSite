@model IEnumerable<UI.Models.SchoolPartyViewModel>

@{
    ViewBag.Title = "Index";
}

@foreach (var item in Model)
{
    <div class="card-group">
        <div class="card">
            <img src="@Html.DisplayFor(modelItem => item.ImageLink)" class="card-img-top" width="250" height="250" alt="...">
            <div class="card-body">
                <h5 class="card-title">@Html.DisplayFor(modelItem => item.FullDate)</h5>
                <p class="card-text">@Html.DisplayFor(modelItem => item.Description)</p>
            </div>
        </div>
    </div>
}